package mkproject.maskat.Papi;

import java.util.List;

import org.bukkit.inventory.ItemStack;

public class PapiItemUtils {
	protected static void addLore(ItemStack itemStack, List<String> loresList) {
		ItemStack itemStack = new ItemStack(material, 1);
		ItemMeta itemMeta = itemStack.getItemMeta();
		List<String> itemLores = itemMeta.getLore();
		for(lore : loresList)
			itemLores.add(lore);
		itemLores.add("&2Kup za &a"+price+"&2 "+currency);
		itemMeta.setLore(itemLores);
		itemStack.setItemMeta(itemMeta);
	}
}
