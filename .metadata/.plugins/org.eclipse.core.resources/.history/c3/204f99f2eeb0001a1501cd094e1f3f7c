name: CommandBlocker
main: me.maskat.CommandBlocker.Plugin
description: Command Blocker
version: 1.0.0
author: MasKAT
website: http://skyidea.pl/
api-version: 1.15
load: POSTWORLD
depend: [MKProjectAPI]

permissions:
   mkp.commandblocker.default:
      description: Allows use default commands
      default: op
   mkp.commandblocker.vip:
      description: Allows use vip commands
      default: op
   mkp.commandblocker.admin:
      description: Allows use admin commands
      default: op
   mkp.commandblocker.bypassall:
      description: Allows use all commands
      default: op